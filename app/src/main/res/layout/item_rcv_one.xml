<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="20dp">

    <ImageView
        android:id="@+id/img_item_menu_one_1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/bg_img_item_menu_one"
        android:padding="9dp"
        android:src="@drawable/icons_8_alarm"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:id="@+id/tv_item_menu_one_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/raleway_medium"
        android:text="Alerts"
        android:textColor="#454545"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@id/img_item_menu_one_1"
        app:layout_constraintTop_toTopOf="parent"></TextView>

    <ImageView
        android:id="@+id/img_item_menu_one_2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginVertical="15dp"
        android:layout_marginLeft="45dp"
        android:background="@drawable/bg_img_item_menu_one"
        android:padding="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img_item_menu_one_1"></ImageView>

    <TextView
        android:id="@+id/tv_item_menu_one_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/raleway_medium"
        android:text="Alerts"
        android:textColor="#454545"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@id/img_item_menu_one_2"
        app:layout_constraintTop_toBottomOf="@id/img_item_menu_one_1"></TextView>

    <ImageView
        android:id="@+id/img_item_menu_one_3"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginVertical="15dp"
        android:layout_marginLeft="45dp"
        android:background="@drawable/bg_img_item_menu_one"
        android:padding="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img_item_menu_one_2"></ImageView>

    <TextView
        android:id="@+id/tv_item_menu_one_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/raleway_medium"
        android:text="Alerts"
        android:textColor="#454545"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@id/img_item_menu_one_3"
        app:layout_constraintTop_toBottomOf="@id/img_item_menu_one_2"></TextView>

    <ImageView
        android:id="@+id/img_item_menu_one_4"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginVertical="15dp"
        android:layout_marginLeft="45dp"
        android:background="@drawable/bg_img_item_menu_one"
        android:padding="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img_item_menu_one_3"></ImageView>

    <TextView
        android:id="@+id/tv_item_menu_one_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/raleway_medium"
        android:text="Alerts"
        android:textColor="#454545"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@id/img_item_menu_one_4"
        app:layout_constraintTop_toBottomOf="@id/img_item_menu_one_3"></TextView>
</androidx.constraintlayout.widget.ConstraintLayout>